results
{
    minMax
    {
        scalar
        {
            min(mag(U))     0;
            max(mag(U))     3.913106853539237;
        }
        vector
        {
            min(mag(U))_position ( -0.0110006 -0.0110006 0 );
            max(mag(U))_position ( 0 0 0.0198 );
        }
        label
        {
            min(mag(U))_processor 0;
            max(mag(U))_processor 0;
        }
    }
