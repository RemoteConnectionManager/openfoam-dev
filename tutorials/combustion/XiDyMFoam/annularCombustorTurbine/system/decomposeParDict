FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 6;

method          hierarchical;

hierarchicalCoeffs
{
    n               ( 6 1 1 );
    delta           0.001;
    order           xyz;
}

preservePatches (cyclic0 cyclic1 ami0 ami1);


